AAA,CAAA;AAAA,eAAA,CAAA,IAAA,CAAA,CAAA,MAAA,IAAA,EAAA,IAAA,sCAAA;AAAA,OAFA,CAPA;AAWA,MAAA,KAAA,EAAA,EAXA;AAYA,MAAA,UAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,oBAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,YAAA,IAAA,CAAA,CAAA,KAAA,sBAAA;AAAA,OAFA,CAZA;AAgBA,MAAA,IAAA,EAAA,EAhBA;AAiBA,MAAA,SAAA,EAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,CAAA,IAAA,kBAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,CAAA,CAAA,MAAA,IAAA,EAAA,IAAA,oBAAA;AAAA,OAFA,CAjBA;AAqBA,MAAA,KAAA,EAAA,IArBA;AAsBA,MAAA,IAAA,EAAA,IAtBA;AAuBA,MAAA,KAAA,EAAA,EAvBA;AAwBA,MAAA,UAAA,EAAA,CACA,qBADA,EAEA,qBAFA,EAGA,qBAHA,EAIA,qBAJA,EAKA,YALA,CAxBA;AA+BA,MAAA,SAAA,EAAA,CACA,eADA,EAEA,eAFA,EAGA,eAHA,EAIA,eAJA,EAKA,eALA,EAMA,eANA,EAOA,eAPA,EAQA,SARA,CA/BA;AAyCA,MAAA,QAAA,EAAA,KAzCA;AA0CA,MAAA,MAAA,EAAA,IAAA,IAAA,GAAA,WAAA,GAAA,MAAA,CAAA,CAAA,EAAA,EAAA;AA1CA,KAAA;AAAA,GAJA;AAiDA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,MADA,EACA;AACA,WAAA,MAAA,GAAA,KAAA;AAEA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,KAAA,MAAA,CAAA;AAEA,WAAA,WAAA,aAAA,IAAA,CAAA,YAAA,EAAA,gBAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,WAAA;AACA,KARA;AASA,IAAA,QATA,sBASA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,KAXA;AAYA,IAAA,KAZA,mBAYA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,KAdA;AAeA,IAAA,eAfA,6BAeA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA;AAjBA;AAjDA,CAAA","sourcesContent":["<template>\n  <v-container class=\"mx-auto my-10\">\n    <v-card raised shaped max-width=\"600px\" class=\"mx-auto px-4 py-3\">\n      <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n        <v-container>\n          <v-card flat>\n            <v-row>\n              <v-col cols=\"12\" sm=\"6\">\n                <v-text-field\n                  v-model=\"first_name\"\n                  :counter=\"15\"\n                  :rules=\"nameRules\"\n                  label=\"First Name\"\n                  required\n                ></v-text-field\n              ></v-col>\n              <v-col cols=\"12\" sm=\"6\">\n                <v-text-field\n                  v-model=\"last_name\"\n                  :counter=\"15\"\n                  :rules=\"nameRules\"\n                  label=\"Last Name\"\n                  required\n                ></v-text-field\n              ></v-col>\n            </v-row>\n\n            <v-text-field\n              v-model=\"email\"\n              :rules=\"emailRules\"\n              label=\"E-mail\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"phone\"\n              :rules=\"nameRules\"\n              label=\"Phone Number\"\n              :counter=\"10\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"city\"\n              :counter=\"25\"\n              :rules=\"cityRules\"\n              label=\"Potential City\"\n              required\n            ></v-text-field>\n            <v-row>\n              <v-col cols=\"6\">\n                <v-select\n                  v-model=\"price\"\n                  :items=\"priceRange\"\n                  :rules=\"[(v) => !!v || 'Item is required']\"\n                  label=\"Price Range\"\n                  required\n                ></v-select>\n              </v-col>\n              <v-col cols=\"6\">\n                <v-select\n                  v-model=\"size\"\n                  :items=\"sqFootage\"\n                  :rules=\"[(v) => !!v || 'Item is required']\"\n                  label=\"Sq. Footage\"\n                  required\n                ></v-select\n              ></v-col>\n            </v-row>\n\n            <div>\n              <v-row>\n                <v-col cols=\"9\">\n                  <v-text-field\n                    label=\"Date\"\n                    v-model=\"appointment\"\n                    readonly\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"3\">\n                  <v-dialog\n                    v-model=\"dialog\"\n                    transition=\"dialog-bottom-transition\"\n                    persistent\n                  >\n                    <template v-slot:activator=\"{ on, attrs }\"\n                      ><v-btn\n                        class=\"primary white--text mt-3\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                        ><v-icon>mdi-calendar-clock</v-icon></v-btn\n                      ></template\n                    >\n                    <template v-slot:default>\n                      <v-sheet color=\"white\">\n                        <v-date-picker\n                          :min=\"today\"\n                          full-width\n                          v-model=\"picker\"\n                        ></v-date-picker>\n                        <div class=\"\">\n                          <time-picker @save=\"saveDate\"></time-picker>\n                        </div>\n                      </v-sheet>\n                    </template>\n                  </v-dialog>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"12\">\n                  <v-btn class=\"primary\">Submit</v-btn>\n                </v-col>\n              </v-row>\n            </div>\n          </v-card>\n        </v-container>\n      </v-form>\n    </v-card>\n  </v-container>\n</template>\n\n<script>\nfunction minDate() {\n  Date.prototype.addDays = function(days) {\n    var date = new Date(this.valueOf());\n    date.setDate(date.getDate() + days);\n    return date;\n  };\n  const date = new Date()\n    .addDays(1)\n    .toISOString()\n    .substring(0, 10);\n  return date;\n}\nimport TimePicker from \"./TimePicker.vue\";\nexport default {\n  components: {\n    TimePicker,\n  },\n  data: () => ({\n    today: minDate(),\n    dialog: false,\n    appointment: null,\n    valid: true,\n    first_name: \"\",\n    last_name: \"\",\n    nameRules: [\n      (v) => !!v || \"Name is required\",\n      (v) => (v && v.length <= 15) || \"Name must be less than 10 characters\",\n    ],\n    email: \"\",\n    emailRules: [\n      (v) => !!v || \"E-mail is required\",\n      (v) => /.+@.+\\..+/.test(v) || \"E-mail must be valid\",\n    ],\n    city: \"\",\n    cityRules: [\n      (v) => !!v || \"City is required\",\n      (v) => (v && v.length <= 25) || \"City must be valid\",\n    ],\n    price: null,\n    size: null,\n    phone: \"\",\n    priceRange: [\n      \"$200,000 - $300,000\",\n      \"$300,000 - $400,000\",\n      \"$400,000 - $500,000\",\n      \"$500,000 - $600,000\",\n      \"$600,000 +\",\n    ],\n    sqFootage: [\n      \"1,000 - 1,250\",\n      \"1,250 - 1,500\",\n      \"1,500 - 1,800\",\n      \"1,800 - 2,000\",\n      \"2,000 - 2,500\",\n      \"2,500 - 3,000\",\n      \"3,000 - 4,000\",\n      \"4,000 +\",\n    ],\n    checkbox: false,\n    picker: new Date().toISOString().substr(0, 10),\n  }),\n\n  methods: {\n    saveDate($event) {\n      this.dialog = false;\n\n      const date = new Date(this.picker);\n\n      this.appointment = `${date.toDateString()} @ ${$event} `;\n      console.log(this.appointment);\n    },\n    validate() {\n      this.$refs.form.validate();\n    },\n    reset() {\n      this.$refs.form.reset();\n    },\n    resetValidation() {\n      this.$refs.form.resetValidation();\n    },\n  },\n};\n</script>\n\n<style>\n.time-select {\n  padding-top: 12px;\n  margin-top: 4px;\n}\n.time-select label {\n  margin-top: -50px;\n  margin-right: -28px;\n}\n.time-hours,\n.time-minutes {\n  width: 40px;\n  min-width: 40px !important;\n  height: 32px !important;\n}\n.time-hours .v-btn__content i.v-icon,\n.time-minutes .v-btn__content i.v-icon {\n  margin-left: -4px;\n}\n\n.time-select .toggle-am-pm .v-btn.v-size--default {\n  width: 36px;\n  min-width: 36px !important;\n  height: 32px !important;\n  padding-left: 0;\n  padding-right: 0;\n}\n</style>\n"],"sourceRoot":"src/components"}]}